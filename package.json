{
  "name": "react-boilerplate-app",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "author": "Olawale Adeleye",
  "license": "MIT",
  "scripts": {
    "prestart": "echo \"Staring up application.\"",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/test-setup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "bootstrap": "3.3.6",
    "jquery": "2.2.3",
    "prop-types": "^15.5.6",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "6.8.0",
    "css-loader": "^0.28.0",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "^4.13.4",
    "file-loader": "0.8.5",
    "jsdom": "8.5.0",
    "mocha": "2.4.5",
    "npm-run-all": "1.8.0",
    "react-hot-loader": "^1.3.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.16.1",
    "url-loader": "0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/walexee/React-Boilerplate"
  }
}
